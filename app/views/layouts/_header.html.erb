<section>
  <div class="ed-top">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="ed-com-t1-left">
            <ul>
              <li>
                <a href=""><%= t "home.header.address" %></a>
              </li>
              <li>
                <a href=""><%= t "home.header.phone" %></a>
              </li>
            </ul>
          </div>
          <div class="ed-com-t1-right">
            <ul>
              <li class="cart">
                <%= image_tag "icon1.png" %>
                <%= link_to(carts_path)  do %>
                  <number class="number-cart"><%= @count_dish_cart %></number>
                <% end %>
              </li>
              <li class="book-table">
                <div class="datban-boder">
                  <%= image_tag "icon2.png" %>
                  <%= link_to t("home.header.book_now"), tables_path %>
                </div>
              </li>
              <% if logged_in? %>
                <li class="book-user">
                  <%= link_to current_user.name, edit_user_path(current_user),
                    {class: "log"} %>
                </li>
                <li class="book-user">
                  <%= link_to t("users.logout"), logout_path, method: :delete %>
                </li>
              <% else %>
                <li class="book-user">
                  <%= link_to t("home.header.sign_up"), new_user_path %>
                </li>
                <li class="book-user">
                  <%= link_to t("home.header.login"), login_path %>
                </li>
              <% end %>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="top-logo">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="wed-logo">
            <%= link_to root_path do %>
              <%= image_tag "logo.png" %>
            <% end %>
          </div>
          <div class="main-menu">
            <ul>
              <li>
                <%= link_to t("home.home"), root_path %>
              </li>
              <li class="cour-menu">
                <%= link_to t("home.header.menu"), "", class: "link_to_menu" %>
                <div class="mm-pos">
                  <div class="cour-mm m-menu">
                    <div class="m-menu-inn">
                      <%= load_menu_template(@menu).html_safe %>
                    </div>
                  </div>
                </div>
              </li>
              <li>
                <a href="" id="seach"><i class="glyphicon glyphicon-search"></i></a>
                <div class="search">
                  <%= form_tag dishes_path, method: :get do %>
                    <%= text_field_tag :search, params[:search],
                      placeholder: t("home.search"), id: "search_auto_complete",
                      class: "form-control" %>
                  <% end %>
                  <div class="result_search">
                  </div>
                </div>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<%= render "shared/flash" %>
<div class="swiper-container">
  <div class="swiper-wrapper">
    <div class="swiper-slide">
      <div class="swiper-container swiper-container-v">
        <div class="swiper-wrapper">
          <div class="swiper-slide">
            <%= image_tag "bcdc271015f82896842a14b78c0dac64.jpg" %>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="search_table">
    <div class="center find_table_parent">
      <h2><%= t "home.find_table" %></h2>
    </div>
    <div class="form__find_table">
      <%= form_tag tables_path, method: :get do %>
        <div class="col-md-5">
          <%= number_field_tag :number, params[:search], required: true,
            min: 1, class: "form-control" %>
        </div>
        <div class="col-md-5">
          <%= select_tag :status, options_for_select(@type_tables), {class: "form-control"} %>
        </div>
        <div class="col-md-2">
          <%= button_tag type: "submit", class: "btn btn-primary" do %>
            <%= t "Search" %>
          <% end %>
        </div>
      <% end %>
    </div>
  </div>
</div>
<section id="info-contact-pro-main">
  <div class="container">
    <div class="col-xs-4 col-sm-4 col-md-3 col-lg-4">
      <div class="menu-img-pro-info">
        <%= image_tag "img1.png" %>
        <p>
          <%= t "home.exlore" %>
          <%= t "home.food_menu" %>
        </p>
      </div>
    </div>
    <div class="col-xs-12 col-sm-3 col-md-4 col-lg-3">
      <div class="item-info-banner">
        <div class="fix-width-info">
          <%= image_tag "icon3.png" %>
          <p>
            <%= t "home.see" %><br/>
            <span class="under_slide--weight"><%= t "rating" %></span>
          </p>
          <div class="fix-mob">
            <%= image_tag "icon15.png" %>
          </div>
        </div>
      </div>
    </div>
    <div class="col-xs-12 col-sm-3 col-md-4 col-lg-3">
      <div class="item-info-banner2">
        <div class="fix-width-info">
          <%= image_tag "icon4.png" %>
          <p><%= t "home.call" %></p>
          <h2><%= t "home.home_deliver" %></h2>
        </div>
      </div>
    </div>
    <div class="col-xs-2 col-sm-2 col-md-1 col-lg-2">
      <div class="item-info-banner3">
        <h2><%= t "home.phone" %></h2>
      </div>
    </div>
  </div>
</section>
